{
    "name": "nonsapiens/addressfactory",
    "type": "library",
    "description": "Creates real-world street addresses from Google Maps, to use in database seeding, unit tests, or anything else. Supports Laravel 8+, and Faker",
    "keywords": [
        "AddressFactory"
    ],
    "homepage": "https://github.com/nonsapiens/addressfactory",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Steedman",
            "email": "stuart@yonder.co.za",
            "homepage": "http://www.yonder.co.za",
            "role": "Head of Development"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "fakerphp/faker": "^1.13",
        "illuminate/support": "^8.27",
        "laravel/framework": "^8.0",
        "toin0u/geocoder-laravel": "^4.4"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Nonsapiens\\AddressFactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nonsapiens\\AddressFactory\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nonsapiens\\AddressFactory\\AddressFactoryServiceProvider"
            ],
            "aliases": {
                "AddressFactory" : "Nonsapiens\\AddressFactory\\Facades\\AddressFactory"
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
